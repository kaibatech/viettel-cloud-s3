{
    "name": "kaibatech/viettel-cloud-s3",
    "description": "Viettel Cloud Object Storage - Laravel Storage Driver for VIPCore/EMC ViPR S3-compatible endpoints",
    "keywords": [
        "laravel",
        "storage",
        "s3",
        "viettel",
        "cloud",
        "vipcore",
        "emc",
        "vipr",
        "flysystem"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hoang Stark",
            "email": "hoang@kaibatech.com"
        }
    ],
    "homepage": "https://github.com/kaibatech/viettel-cloud-s3",
    "support": {
        "issues": "https://github.com/kaibatech/viettel-cloud-s3/issues",
        "source": "https://github.com/kaibatech/viettel-cloud-s3"
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0",
        "league/flysystem": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Kaibatech\\ViettelCloudS3\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaibatech\\ViettelCloudS3\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaibatech\\ViettelCloudS3\\ViettelCloudS3ServiceProvider"
            ],
            "aliases": {
                "ViettelCloudS3": "Kaibatech\\ViettelCloudS3\\Facades\\ViettelCloudS3"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
} 